{
  "name": "zonky-api-handler",
  "version": "0.1.0",
  "description": "Unofficial handler for Zonky API",
  "main": "./dist/index.js",
  "jsnext:main": "dist/index.es.js",
  "module": "dist/index.es.js",
  "scripts": {
    "lint": "eslint --ext .js ./src ./tests",
    "lint-export": "npm run lint -- -o ./tests_results/checkstyle/js-checkstyle.xml -f checkstyle",
    "flow-init": "flow-typed install jest@22.x luxon@0.4.x",
    "flow": "flow",
    "flow-coverage": "flow-coverage-report -f \"node node_modules/flow-bin/vendor/flow\" -i ./src/**/*.js* -t html -t text -t json -o tests_results/coverage/flow",
    "build": "rm -rf ./dist && rollup --config ./rollup.config.js",
    "test": "jest",
    "test-build": "npm run test -- --config jest.config.build.js",
    "precommit": "npm run js-lint && npm run flow && npm run test",
    "prepublishOnly": "npm run build && npm run test-build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/fabulator/zonky-api-handler.git"
  },
  "author": "Michal Ozog√°n <michal@ozogan.eu>",
  "license": "Apache License 2.0",
  "homepage": "https://github.com/fabulator/zonky-api-handler#readme",
  "devDependencies": {
    "cross-fetch": "^2.1.1",
    "@socifi/eslint-config": "^1.8.1",
    "flow-bin": "^0.73.0",
    "flow-coverage-report": "^0.5.0",
    "flow-typed": "^2.3.0",
    "@socifi/jest-config": "^1.9.0",
    "@socifi/rollup-config": "^1.10.0"
  },
  "dependencies": {
    "luxon": "^1.2.1",
    "rest-api-handler": "^1.8.0"
  }
}
